{
	"version": 2,
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "dist"
		}
	  },
	  {
		"src": "public/api/db.json",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "/api"
	  },
	  {
		"src": "/(.*)",
		"dest": "/index.html"
	  }
	]
  }